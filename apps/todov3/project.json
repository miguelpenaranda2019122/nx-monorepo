{
  "name": "todov3",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/todov3/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "run-android": {
      "executor": "@nx/react-native:run-android",
      "options": {
        "resetCache": true
      },
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "interactive": true,
          "activeArchOnly": true
        },
        "staging": {
          "interactive": true,
          "activeArchOnly": true
        },
        "production": {
          "interactive": true,
          "activeArchOnly": true
        }
      }
    },
    "run-ios": {
      "executor": "@nx/react-native:run-ios",
      "options": {
        "resetCache": true
      },
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "interactive": true
        },
        "staging": {
          "interactive": true
        },
        "production": {
          "interactive": true
        }
      }
    }
  }
}
